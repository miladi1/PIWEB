                    {%block content %}




                    <html>

                    <head>
                        <meta charset="UTF-8"/>
                        <script language="javascript">


                            function envoyer(){
                                const url="https://api.twilio.com/2010-04-01/Accounts/ACe0b24554430af176a4b4950be4f5f5d0/Messages.json";
                                const auth="ACe0b24554430af176a4b4950be4f5f5d0:069adb3561b6e54431567be757a88b6c";

                                const message=document.getElementById("message").value;
                                const numTel=document.getElementById("mobile").value;

                                const msg=encodeURI(message);
                                const num=encodeURI(numTel);


                                const myHeader=new Headers({
                                    'Content-Type':'application/x-www-form-urlencoded',
                                    'Authorization':'Basic '+btoa(auth)

                                });
                                const init={
                                    method:'POST',
                                    headers : myHeader ,
                                    mode:'cors',
                                    body:"To="+num+"&From=+19255216485&Body="+msg


                                }

                                fetch(url,init)
                                    .then(Response=>console.log(Response))
                                    .catch(error=>console.log(console.error));
                            }
                        </script>
                    </head>

                    <body>



                    <div id="app">
                        <form action="{{path('sms')}}">
                            <h1>Send message</h1>

                            <br>
                            <table border="1" >
                                <tr>
                                    <td><label>Enter Mobile Number</label></td>
                                    <td>

                                        <input type="text"   class="form-control" id="mobile" value="+216" required>

                                </tr>
                                <tr>
                                    <td><label>Enter message</label></td>
                                    <td><textarea type="textarea"   class="form-control" id="message" rows="6" required></textarea>
                                </tr>

                                <tr>
                                    <td>


                                        <button type="Submit"  onclick="envoyer();" class="btn btn-primary">Send Message</button>
                    </div>
                </div>
            </div>


</body>

